pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Build World'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Deploy World'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Test World'
            }
        }
        
        stage('Release') {
            steps {
                echo 'Release World'
            }
        }
    }
}
